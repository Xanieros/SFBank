<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="ApplyLoan">
    <default-activity>LoanType</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="LoanType">
      <train-stop/>
    </view>
    <view id="UserInfo">
      <train-stop/>
    </view>
    <view id="LoanApplication">
      <train-stop/>
    </view>
    <task-flow-return id="Homepage">
      <outcome>
        <name>Homepage</name>
      </outcome>
    </task-flow-return>
    <train/>
    <control-flow-rule id="__1">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>Homepage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>LoanApplication</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>submit</from-outcome>
        <to-activity-id>Homepage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
